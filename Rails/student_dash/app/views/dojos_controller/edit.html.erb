<div class="container">
  <div class="jumbotron">
    <% if flash[:errors] %>
        <div>
          <p><%= flash[:errors] %></p>
      </div>
      <% end %>

    <h2>New Dojo</h2>
    <form class="form-horizontal" action="/dojos/<%= @show.id %>" method="post">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <input type="hidden" name="_method" value="patch">
      <div class="form-group">
        <label class="control-label col-sm-2" >Branch:</label>
        <div class="col-sm-10">
          <input type="text" class="form-control"  name= "cdBranch[branch]" value="<%= @show.branch %>">
        </div>
      </div>
      <div class="form-group">
        <label class="control-label col-sm-2" >Street:</label>
        <div class="col-sm-10">
          <input type="text" class="form-control"  name="cdBranch[street]" value="<%= @show.street %>">
        </div>
      </div>
      <div class="form-group">
        <label class="control-label col-sm-2" >Street:</label>
        <div class="col-sm-10">
          <input type="text" class="form-control"  name="cdBranch[city]" value="<%= @show.city %>">
        </div>
      </div>
      <div class="form-group">
        <label class="control-label col-sm-2" >State:</label>
        <div class="col-sm-10">
          <input type="text" class="form-control" name="cdBranch[state]" value="<%= @show.state %>">
        </div>
      </div>
      <input type="submit" value="update">
    </form>

  <a href="/">Go Back</a>


  </div>

</div>
