<div class="container">
  <div class="jumbotron">
    <% if flash[:errors] %>
        <div>
          <p><%= flash[:errors] %></p>
      </div>
      <% end %>

    <h2>New Dojo</h2>
    <form class="form-horizontal" action="/dojos" method="post">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
      <div class="form-group">
        <label class="control-label col-sm-2" >Branch:</label>
        <div class="col-sm-10">
          <input type="text" class="form-control"  name= "cdBranch[branch]" placeholder="Branch">
        </div>
      </div>
      <div class="form-group">
        <label class="control-label col-sm-2" >Street:</label>
        <div class="col-sm-10">
          <input type="text" class="form-control"  name="cdBranch[street]" placeholder="Street">
        </div>
      </div>
      <div class="form-group">
        <label class="control-label col-sm-2" >Street:</label>
        <div class="col-sm-10">
          <input type="text" class="form-control"  name="cdBranch[city]" placeholder="City">
        </div>
      </div>
      <div class="form-group">
        <label class="control-label col-sm-2" >State:</label>
        <div class="col-sm-10">
          <input type="text" class="form-control" name="cdBranch[state]" placeholder="State">
        </div>
      </div>
      <input type="submit" value="Create">
    </form>

  <a href="/">Go Back</a>
  

  </div>

</div>
